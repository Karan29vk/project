								<td><%= link_to '' ,add_task_path , class: "icon-pencil" %></td>
							</tr>
							<tr>
								<td>
									<%= link_to 'Follow-up for new project' , nil  %>
								</td>
								<td><%= link_to 'Mayur Shah' , nil  %></td>
